/******  ON LOAD SET UP STUFF *********/

$(document).ready(function() {

  // show lang dialog if the URL includes /intl/
  //if (location.pathname.substring(0,6) == "/intl/") {
  //  var lang = location.pathname.split('/')[2];
   // if (lang != getLangPref()) {
